with [get-editor "Test Profile Diagram"] {
    with [get-diagram -index 1 | get-edit-part -name "Test Profile Diagram"] {
        with [get-edit-part -name "«Metaclass»\n"
            + "Class" | get-target-connection 0] {
            mouse-move 43 73 button1 -height 192 -width 64
            mouse-press 43 73 button1 -height 192 -width 64
            mouse-release 43 73 button1 524288 -height 192 -width 64
        }
    }
    get-button "Delete from Model" | click
}
// Check diagram
get-editor "Test Profile Diagram" | get-diagram -index 1 | get-edit-part -name "Test Profile Diagram"
    | get-edit-part -name "«Metaclass»\n"
            + "Class" | verify-error {get-target-connection 0}
// CheckModel
get-editor "Test Profile Diagram" | get-diagram -index 1 | get-edit-part -name "Test Profile Diagram" 
    | get-property "element.target.member.size" | equals 4 | verify-true